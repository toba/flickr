{
   "name": "@toba/flickr",
   "description": "Toba Flickr API",
   "version": "3.0.0",
   "private": false,
   "publishConfig": {
      "access": "public"
   },
   "author": {
      "name": "Jason Abbott"
   },
   "license": "MIT",
   "main": "cjs/index.js",
   "module": "esm/index.js",
   "types": "esm/index.d.ts",
   "files": [
      "esm",
      "cjs"
   ],
   "scripts": {
      "build:esm": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.esm.json",
      "build:cjs": "./node_modules/.bin/tsc --project ./node_modules/@toba/develop/tsconfig.cjs-min.json",
      "prepublishOnly": "npm run build:esm & npm run build:cjs",
      "test": "jest --coverage"
   },
   "devDependencies": {
      "@toba/develop": "^6.6.11",
      "@toba/test": "^4.1.0",
      "@types/node-fetch": "^2.1.0",
      "typescript": "^3.5.0"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/toba/flickr"
   },
   "bugs": {
      "url": "https://github.com/toba/flickr/issues"
   },
   "dependencies": {
      "@toba/node-tools": "^1.4.0",
      "@toba/oauth": "^2.1.0",
      "node-fetch": "^2.1.0"
   }
}
